{
   "name": "service",
   "version": "1.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "build": "./remove-erroneous-files.sh && pnpm install && tsoa spec-and-routes && tsc",
      "dev": "tsc && node ./build/server.js",
      "pre-commit": "./remove-intermediate-files.sh && pnpm install && pnpm build && pnpm test:coverage",
      "preinstall": "npx only-allow pnpm",
      "test": "pnpm test:micro && pnpm test:integration",
      "test:coverage": "jest --coverage --passWithNoTests --testMatch '**/?(*.)+(micro|integration).[jt]s?(x)'",
      "test:integration": "jest --passWithNoTests --testMatch '/work/service/src/**/*.integration.ts'",
      "test:micro": "jest --passWithNoTests --testMatch '/work/service/src/**/*.micro.ts'",
      "upgrade-all": "pnpm upgrade:packages && pnpm upgrade:fix-vulnerabilities && pnpm pre-commit",
      "upgrade:packages": "pnpx npm-check-updates -u",
      "upgrade:fix-vulnerabilities": "pnpm audit fix || echo \"Could not resolve some vulnerabilities\""
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "dependencies": {
      "@jest/globals": "29.6.2",
      "@tsoa/runtime": "5.0.0",
      "axios": "1.4.0",
      "body-parser": "1.20.2",
      "cors": "2.8.5",
      "decimal.js": "10.4.3",
      "expect": "29.6.2",
      "express": "4.18.2",
      "express-actuator": "1.8.4",
      "inversify": "6.0.1",
      "inversify-binding-decorators": "4.0.0",
      "reflect-metadata": "0.1.13",
      "swagger-ui-express": "5.0.0",
      "ts-luxon": "4.3.2",
      "tsoa": "5.1.1",
      "winston": "3.10.0"
   },
   "devDependencies": {
      "@jest-mock/express": "2.0.2",
      "@types/cors": "2.8.13",
      "@types/express": "4.17.17",
      "@types/jest": "29.5.3",
      "@types/node": "20.5.0",
      "@types/supertest": "2.0.12",
      "jest": "29.6.2",
      "nodemon": "3.0.1",
      "supertest": "6.3.3",
      "ts-jest": "29.1.1",
      "ts-node": "10.9.1",
      "tslib": "2.6.1",
      "typescript": "5.1.6"
   }
}
