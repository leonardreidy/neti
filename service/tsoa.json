{
   "_comment": "https://tsoa-community.github.io/docs/di.html",
   "entryFile": "src/server.ts",
   "noImplicitAdditionalProperties": "throw-on-extras",
   "controllerPathGlobs": [
      "src/**/*Controller.ts"
   ],
   "spec": {
      "outputDirectory": "tsoa-generated",
      "specVersion": 3,
      "securityDefinitions": {
         "custom": {
            "type": "http",
            "name": "Authorization",
            "in": "header",
            "scheme": "bearer",
            "bearerFormat": "JWT"
         }
      }
   },
   "routes": {
      "routesDir": "tsoa-generated",
      "iocModule": "src/config/ioc"
   }
}
